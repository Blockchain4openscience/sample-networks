/**
 * My commodity trading network
 */
namespace org.acme.cite
asset BibRef identified by bibRefId {
    o String bibRefId
    o String description
    o Integer citations
    --> Researcher owner
}
participant Researcher identified by resId {
    o String resId
    o String firstName
    o String lastName
}
transaction Cite {
    --> BibRef bibRef
    --> Researcher citer
}

